block vars

include ../_mixin/_common.pug
include ../_mixin/_svg.pug

doctype html
html(lang="ja")
  head
    include ./_head.pug
  body
    .l-page.js-pjax-page(data-page-id=page.id)
      .js-pjax-fixed-before
        block contentFixedBefore
      .l-contents
        .js-pjax-contents
          block contentStatic
      .p-contents-overlay.js-pjax-overlay.is-expand
      .p-progress.js-pjax-progress
        .p-progress__marker
      .p-dummy-scroll.js-dummy-scroll
      .js-pjax-fixed-after
        block contentFixedAfter
      include ./_gf.pug
    block script
    script(src="/js/main.js")
